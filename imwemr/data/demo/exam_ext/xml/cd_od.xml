			<inf_rim examname="Inferior Rim">
				<intact elem_name="elem_onOd_irim_Intact"></intact>
				<thin elem_name="elem_onOd_irim_Thin"></thin>
				<cupd2_rim elem_name="elem_onOd_irim_c2rim"></cupd2_rim>
			</inf_rim>
			
			<opt_nrv_hmg examname="Optic Nerve">			
				<at_clock elem_name="elem_onOd_onhmg"></at_clock>
			</opt_nrv_hmg>			
