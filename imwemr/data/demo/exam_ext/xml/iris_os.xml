<iris>
	<os>
		<pi examname="PI">
			<open elem_name="elem_piOs_open"></open>
			<close elem_name="elem_piOs_close"></close>
		</pi>
		<iridectomy examname="Iridectomy">
			<peripheral elem_name="elem_iridectomyOs_Iridectomy"></peripheral>
			<sector elem_name="elem_iridectomyOs_sector"></sector>
		</iridectomy>
		<synechia examname="Synechiae">
			<text elem_name="elem_synechiaOs_text"></text>
			<anterio examname="Anterior" examname_summary="Anterior Synechiae">				
				<absent elem_name="elem_synechiaAnterioOs_neg"></absent>
				<present elem_name="elem_synechiaAnterioOs_pos"></present>
			</anterio>
			<posterior examname="Posterior" examname_summary="Posterior Synechiae">				
				<absent elem_name="elem_synechiaPosteriorOs_neg"></absent>
				<present elem_name="elem_synechiaPosteriorOs_pos"></present>
			</posterior>
		</synechia>
		<Nevus examname="Nevus" commonexam="1">
			<negative elem_name="elem_irisNevusOs_neg"></negative>
			<present elem_name="elem_irisNevusOs_Pos"></present>
			<Inferior elem_name="elem_irisNevusOs_Inferior"></Inferior>
			<Superior elem_name="elem_irisNevusOs_Superior"></Superior>
			<Temporal elem_name="elem_irisNevusOs_Temporal"></Temporal>
			<Nasal elem_name="elem_irisNevusOs_Nasal"></Nasal>
		</Nevus>
		<psx examname="PSX" commonexam="1">
			<negative elem_name="elem_psxOs_neg"></negative>			
			<pos_4 elem_name="elem_psxOs_pos4"></pos_4>
			<pos_3 elem_name="elem_psxOs_pos3"></pos_3>
			<pos_2 elem_name="elem_psxOs_pos2"></pos_2>
			<pos_1 elem_name="elem_psxOs_pos1"></pos_1>
			<t elem_name="elem_psxOs_T"></t>
		</psx>
		<nvi examname="NVI">
			<negative elem_name="elem_iris_nviOs_neg"></negative>
			<present elem_name="elem_iris_nviOs_pos"></present>
		</nvi>
		<advanceoptions elem_name="elem_irisAdvanceOptionsOs"></advanceoptions>
	</os>
</iris>